{
  "name": "frontend-future",
  "version": "1.0.0",
  "description": "An investigation into the client-side architecture of Dotcom.",
  "scripts": {
    "start": "nodemon server.js localhost 3000 --exec babel-node --presets latest",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run build:svelte",
    "build:server": "npm run build:svelte:server",
    "build:svelte": "rollup -c prototypes/svelte/rollup.config.js",
    "build:svelte:server": "rollup -c prototypes/svelte/rollup.config.ssr.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardian/frontend-future.git"
  },
  "homepage": "https://github.com/guardian/frontend-future#readme",
  "dependencies": {
    "@polymer/polymer": "^1.2.5-npm-test.2",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "express": "^4.14.0",
    "global": "^4.3.1",
    "hbs": "^4.0.1",
    "help": "^3.0.2",
    "vulcanize": "^1.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.37.0",
    "flow-typed": "^2.0.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-svelte": "^1.0.0",
    "rollup-watch": "^2.5.0"
  }
}
